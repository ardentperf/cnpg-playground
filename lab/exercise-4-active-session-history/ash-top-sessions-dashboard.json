{
  "__inputs": [
    {
      "name": "DS_DS_PROMETHEUS",
      "label": "DS_PROMETHEUS",
      "type": "datasource",
      "pluginId": "prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.3.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series"
    }
  ],
  "id": null,
  "title": "Postgres Active Sessions History",
  "uid": "pg-ash",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 0.6,
            "drawStyle": "bars",
            "fillOpacity": 50,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "interval": "1m",
      "maxDataPoints": 256,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "sum by($dimension) (cnpg_ash_avg_active_sessions{namespace=~\"$namespace\", pod=~\"$instances\", $filter_field=~\".*$filter_text.*\"})",
          "legendFormat": "{{label_name}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Average Active Sessions",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 0.6,
            "drawStyle": "bars",
            "fillOpacity": 10,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "id": 2,
      "interval": "1m",
      "maxDataPoints": 256,
      "options": {
        "legend": {
          "calcs": ["max"],
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "sortBy": "Max",
          "sortDesc": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "max by($dimension) (cnpg_ash_max_by_${dimension}_cnt{namespace=~\"$namespace\", pod=~\"$instances\"})",
          "legendFormat": "{{$dimension}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Max Active Sessions by $dimension",
      "type": "timeseries"
    }
  ],
  "templating": {
    "list": [
      {
        "label": "Datasource",
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "definition": "cnpg_collector_up",
        "description": "Namespace where the database cluster is located",
        "label": "Database Namespace",
        "name": "namespace",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/namespace=\"(?<text>[^\"]+)/g",
        "type": "query"
      },
      {
        "definition": "cnpg_collector_up{namespace=~\"$namespace\"}",
        "label": "Cluster",
        "name": "cluster",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up{namespace=~\"$namespace\"}",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/\\bcluster\\b=\"(?<text>[^\"]+)/g",
        "sort": 1,
        "type": "query"
      },
      {
        "definition": "cnpg_collector_up{namespace=~\"$namespace\",pod=~\"$cluster-([1-9][0-9]*)$\"}",
        "name": "instances",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up{namespace=~\"$namespace\",pod=~\"$cluster-([1-9][0-9]*)$\"}",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/pod=\"(?<text>[^\"]+)/g",
        "sort": 1,
        "type": "query"
      },
      {
        "current": {
          "text": "wait_event",
          "value": "wait_event"
        },
        "label": "Group By",
        "name": "dimension",
        "query": "database_name,user_name,application_name,wait_event,query,query_full,top_level_query,backend_type",
        "type": "custom"
      },
      {
        "current": {
          "text": "query",
          "value": "query"
        },
        "label": "Filter Field",
        "name": "filter_field",
        "query": "database_name,user_name,application_name,wait_event,query,query_full,top_level_query,backend_type",
        "type": "custom"
      },
      {
        "current": {
          "text": "",
          "value": ""
        },
        "description": "Substring to filter the selected field (leave empty to show all)",
        "label": "Filter Text",
        "name": "filter_text",
        "type": "textbox"
      }
    ]
  }
}