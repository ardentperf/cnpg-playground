{
  "__inputs": [
    {
      "name": "DS_DS_PROMETHEUS",
      "label": "DS_PROMETHEUS",
      "type": "datasource",
      "pluginId": "prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "11.3.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series"
    }
  ],
  "id": null,
  "title": "Active Sessions Drill Down - Lab Only Prometheus Killer",
  "uid": "ash-labonly-promkiller",
  "timezone": "browser",
  "refresh": "30s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 0.6,
            "drawStyle": "bars",
            "fillOpacity": 50,
            "gradientMode": "none",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": false,
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "interval": "1m",
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "sum by($dimension) (cnpg_ash_labonly_promkiller_cnt{namespace=~\"$namespace\", pod=~\"$instances\"}) / 60",
          "legendFormat": "{{label_name}}",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Average Active Sessions",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "axisBorderShow": false,
            "axisCenteredZero": false,
            "axisColorMode": "text",
            "axisLabel": "",
            "axisPlacement": "auto",
            "barAlignment": 0,
            "barWidthFactor": 1,
            "drawStyle": "bars",
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "viz": false
            },
            "insertNulls": 1000,
            "lineInterpolation": "linear",
            "lineWidth": 10,
            "pointSize": 5,
            "scaleDistribution": {
              "type": "linear"
            },
            "showPoints": "auto",
            "spanNulls": false,
            "stacking": {
              "group": "A",
              "mode": "normal"
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 10
      },
      "id": 39,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_DS_PROMETHEUS}"
          },
          "editorMode": "code",
          "expr": "sum by(timestamp__destroyer_of_prometheus, $dimension) (cnpg_ash_labonly_promkiller_cnt{namespace=~\"$namespace\", pod=~\"$instances\", $filter_field=~\".*$filter_text.*\"})",
          "legendFormat": "__auto",
          "range": true,
          "refId": "A"
        }
      ],
      "title": "Active Session Drilldown [1 second]",
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {}
        },
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "timestamp_ms",
            "binary": {
              "left": {
                "matcher": {
                  "id": "byName",
                  "options": "timestamp__destroyer_of_prometheus"
                }
              },
              "operator": "*",
              "right": {
                "fixed": "1000"
              }
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "convertFieldType",
          "options": {
            "conversions": [
              {
                "destinationType": "string",
                "targetField": "Time"
              },
              {
                "destinationType": "time",
                "targetField": "timestamp_ms"
              }
            ],
            "fields": {}
          }
        },
        {
          "id": "partitionByValues",
          "options": {
            "fields": [
              "database_name",
              "user_name",
              "application_name",
              "wait_event",
              "query",
              "query_full",
              "top_level_query",
              "backend_type"
            ],
            "keepFields": false,
            "naming": {
              "asLabels": false
            }
          }
        }
      ],
      "type": "timeseries"
    }
  ],
  "templating": {
    "list": [
      {
        "label": "Datasource",
        "name": "DS_PROMETHEUS",
        "query": "prometheus",
        "refresh": 1,
        "type": "datasource"
      },
      {
        "definition": "cnpg_collector_up",
        "description": "Namespace where the database cluster is located",
        "label": "Database Namespace",
        "name": "namespace",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/namespace=\"(?<text>[^\"]+)/g",
        "type": "query"
      },
      {
        "definition": "cnpg_collector_up{namespace=~\"$namespace\"}",
        "label": "Cluster",
        "name": "cluster",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up{namespace=~\"$namespace\"}",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/\\bcluster\\b=\"(?<text>[^\"]+)/g",
        "sort": 1,
        "type": "query"
      },
      {
        "definition": "cnpg_collector_up{namespace=~\"$namespace\",pod=~\"$cluster-([1-9][0-9]*)$\"}",
        "name": "instances",
        "query": {
          "qryType": 4,
          "query": "cnpg_collector_up{namespace=~\"$namespace\",pod=~\"$cluster-([1-9][0-9]*)$\"}",
          "refId": "PrometheusVariableQueryEditor-VariableQuery"
        },
        "refresh": 2,
        "regex": "/pod=\"(?<text>[^\"]+)/g",
        "sort": 1,
        "type": "query"
      },
      {
        "current": {
          "text": "wait_event",
          "value": "wait_event"
        },
        "label": "Group By",
        "name": "dimension",
        "query": "database_name,user_name,application_name,wait_event,query,query_full,top_level_query,backend_type",
        "type": "custom"
      },
      {
        "current": {
          "text": "query",
          "value": "query"
        },
        "label": "Filter Field",
        "name": "filter_field",
        "query": "database_name,user_name,application_name,wait_event,query,query_full,top_level_query,backend_type",
        "type": "custom"
      },
      {
        "current": {
          "text": "",
          "value": ""
        },
        "description": "Substring to filter the selected field (leave empty to show all)",
        "label": "Filter Text",
        "name": "filter_text",
        "type": "textbox"
      }
    ]
  }
}